@model IEnumerable<R2DEV2.Models.CourseClass>

@{
    ViewBag.Title = "Kurser";
}

@foreach (var item in Model)
{
    <h2>@Html.DisplayFor(modelItem => item.CourseName)</h2>
    <div class="col-md-8">Moduler</div>
    <div class="col-md-4">Deltagare</div>
    <td>
        @Html.ActionLink("Redigera", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Detaljer", "Details", new { id = item.Id }) |
        @Html.ActionLink("Ta bort", "Delete", new { id = item.Id })
    </td>
    <hr class="container" />
}

<p>
    @Html.ActionLink("Skapa ny", "Create")
</p>