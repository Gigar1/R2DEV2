@model IEnumerable<R2DEV2.Models.CourseClass>

@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model)
{
    <h2>@Html.DisplayFor(modelItem => item.CourseName)</h2>
    <div class="col-md-8">Moduler</div>
    <div class="col-md-4">Deltagare</div>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
    <hr class="container" />
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>

